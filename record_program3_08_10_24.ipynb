    "#replace occurence of 1st char with $, except the 1st one\n",
    "s = input(\"Enter a string: \")\n",
    "if len(s) == 0:\n",
    "    print(\"Empty string provided\")\n",
    "    \n",
    "fchar = s[0]\n",
    "\n",
    "ns = fchar + s[1:].replace(fchar,'$')\n",
    "\n",
    "print(f\"Modified string is: {ns}\")\n"
   ]
