   "source": [
    "#swap first & last characters of a string\n",
    "s = input(\"Enter a string: \")\n",
    "\n",
    "if len(s) < 2:\n",
    "    print(\"The string is too short to swap characters\")\n",
    "\n",
    "ns = s[-1] + s[1:-1] + s[0]\n",
    "\n",
    "print(f\"New string is: {ns}\")\n"
   ]
  },
